    <%#= f.input :a2 %>
    <%#= f.input :a3 %>
    <%#= f.input :a4 %>
    <%#= f.input :b3 %>
    <%= f.input :b1, label: "Gases" %>
    <%= f.input :b2, label: "Aceite" %>

  </div>
<br>
  <div class="form-actions">
    <%= f.button :submit, "Crear reporte", class: "btn btn-primary shadow" %>
  </div>
<% end %>
