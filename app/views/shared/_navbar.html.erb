    <style>
      .bd-placeholder-img {
        font-size: 1.125rem;
        text-anchor: middle;
        -webkit-user-select: none;
        -moz-user-select: none;
        user-select: none;
      }

      @media (min-width: 768px) {
        .bd-placeholder-img-lg {
          font-size: 3.5rem;
        }
      }

      .b-example-divider {
        height: 3rem;
        background-color: rgba(0, 0, 0, .1);
        border: solid rgba(0, 0, 0, .15);
        border-width: 1px 0;
        box-shadow: inset 0 .5em 1.5em rgba(0, 0, 0, .1), inset 0 .125em .5em rgba(0, 0, 0, .15);
      }

      .b-example-vr {
        flex-shrink: 0;
        width: 1.5rem;
        height: 100vh;
      }

      .bi {
        vertical-align: -.125em;
        fill: currentColor;
      }

      .nav-scroller {
        position: relative;
        z-index: 2;
        height: 2.75rem;
        overflow-y: hidden;
      }

      .nav-scroller .nav {
        display: flex;
        flex-wrap: nowrap;
        padding-bottom: 1rem;
        margin-top: -1px;
        overflow-x: auto;
        text-align: center;
        white-space: nowrap;
        -webkit-overflow-scrolling: touch;
      }
    </style>


<body>
<main>
  <header class="p-3 text-bg-dark noprint">
    <div class="container">
      <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
        <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none">
          <%= image_tag ("logo.svg")%>
        </a>

        <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
          <li><a href="/" class="nav-link px-2 text-white">Inicio</a></li>
          <li><a href="<%= clientes_path %>" class="nav-link px-2 text-white">Clientes</a></li>
          <li><a href="<%= equipos_path %>" class="nav-link px-2 text-white">Equipos</a></li>
          <li><a href="<%= reportes_path %>" class="nav-link px-2 text-white">Reportes</a></li>
          <li><a href="<%= informes_path %>" class="nav-link px-2 text-white">Informes</a></li>

        </ul>

        <div class="text-end">
          <% if user_signed_in? %>
          <strong><%= current_user.email %></strong>
            <%= link_to 'Editar perfil', edit_user_registration_path, :class => 'btn btn-outline-light m-2' %>
            <%= button_to "Salir", destroy_user_session_path, method: :delete, :class => 'btn btn-warning' %>
          <% else %>
            <%= link_to "Registrarse", new_user_registration_path, :class => 'btn btn-outline-light me-2'  %>
            <%= link_to "Ingresar", new_user_session_path, :class => 'btn btn-warning'  %>
          <% end %>

        </div>
      </div>
    </div>
  </header>
</main>
</body>
</html>

<style>
.button_to {
  display: inline;
}

.button_to div {
  display: inline;
}
</style>



